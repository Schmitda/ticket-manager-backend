{
  "name": "angular4-starter-aot",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json --locale=de  ",
    "build": "ng build",
    "build:aot": "ng build --aot -prod --env=prod",
    "build-i18n:fr": "ng build --output-path=dist/fr --aot -prod --bh /fr/ --i18n-file=src/messages.fr.xlf --i18n-format=xlf --locale=fr --env=prod",
    "build-i18n:de": "ng build --output-path=dist/de --aot -prod --bh /de/ --locale=de --env=prod",
    "build-i18n": "npm run build-i18n:de && npm run build-i18n:fr",
    "backend": "nodemon backend/server.js",
    "prod": "SET NODE_ENV=production && forever start backend/server.js",
    "prod-test": "SET NODE_ENV=production && nodemon backend/server.js",
    "prod-linux": "NODE_ENV=production exec forever start backend/server.js",
    "prod-linux-test": "NODE_ENV=production exec nodemon backend/server.js",
    "node-build": "tsc -w -p backend/tsconfig.json",
    "execution": "tsc execution",
    "test": "ng test",
    "fixtures": "node backend/fixtures/Fixtures.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat xlf -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "js-beautify": "^1.7.5",
    "jsonwebtoken": "^8.1.1",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.1",
    "prompt-sync": "^4.1.5",
    "twig": "^1.10.5"
  },
